{
    "name": "karelwintersky/arris.entity",
    "description": "Entity types for Arris µFramework",
    "keywords": [ "entity", "ArrisµFramework", "result" ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Karel Wintersky",
            "email": "karel@wintersky.ru"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "platform-check": false
    },
    "autoload": {
        "psr-4": {
            "Arris\\Entity\\": "sources"
        }
    },
    "require": {
        "php": "^7.4 | 8.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.7 || ^6.5",
        "friendsofphp/php-cs-fixer": "^3"
    }
}
